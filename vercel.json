{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "apps/web/dist"
      }
    }
  ],
  "env": {
    "NODE_VERSION": "20"
  },
  "installCommand": "cd apps/web && npm install",
  "outputDirectory": "apps/web/dist"
}
